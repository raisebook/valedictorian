{
  "name": "valedictorian",
  "version": "1.0.6",
  "description": "Another React validation library",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/raisebook/valedictorian"
  },
  "keywords": [
    "javsacript",
    "reactjs",
    "react",
    "forms",
    "validation"
  ],
  "author": "Myles Eftos <myles@raisebook.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir build --ignore spec.js && cp package.json build/package.json",
    "clean": "rimraf build",
    "lint": "eslint ./",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "yarn run -s test -- --watch"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chai": "^4.0.2",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "jest": "^20.0.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0"
  },
  "peerDependencies": {
    "prop-types": "^15.x",
    "react": "^15.x"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ]
  }
}
